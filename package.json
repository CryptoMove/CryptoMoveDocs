  "description": "[Hello Guide](https://github.com/cryptocharudatta/CryptoMoveDocs/tree/master/source/helloguide.rst)",
  "scripts": {
    "createJson": "yaml2json --save swagger.yaml",
    "deploy": "yarn run createJson && yarn run deployJson",
    "deployJson": "rdme swagger swagger.json --key=$RDME_KY --id=$RDME_ID 1>/dev/null",
    "test": "swagger-spec-validator swagger.yaml"
